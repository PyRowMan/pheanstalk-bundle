{
    "name": "pyrowman/pheanstalk-bundle",
    "description": "The LeezyPheanstalkBundle is a Symfony2 Bundle that provides a command line interface for manage the evqueue workqueue server & a pheanstalk integration.",
    "keywords": ["symfony", "evqueue", "bundle", "pheanstalk", "queueing", "messaging", "asynchronous"],
    "type": "symfony-bundle",
    "homepage": "https://github.com/armetiz/LeezyPheanstalkBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Valentin Corre",
            "email": "vcorre@pielectronique.com",
            "homepage": "http://broken.fr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "psr/log": "~1.0",
        "symfony/framework-bundle": "~2.5|~3.0|^4.0",
        "symfony/console": "~2.5|~3.0|^4.0",
        "pyrowman/pheanstalk": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0|~5.0",
        "phpunit/phpunit-mock-objects": "2.3.0|~3.4"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Pyrowman\\PheanstalkBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pyrowman\\PheanstalkBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
    }
}
